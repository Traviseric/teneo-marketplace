{
  "name": "book-marketplace-backend",
  "version": "1.0.0",
  "description": "Open source bookstore platform - keep 97% vs Amazon's 35%",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup:production": "bash scripts/setup-production.sh",
    "deploy:testnet": "npx hardhat run scripts/deploy-contracts.js --network mumbai",
    "deploy:mainnet": "npx hardhat run scripts/deploy-contracts.js --network polygon",
    "compile:contracts": "npx hardhat compile",
    "init:db": "node database/init.js",
    "init:ai": "node scripts/init-ai-discovery.js",
    "generate:password": "node scripts/generate-password-hash.js"
  },
  "keywords": [
    "bookstore",
    "marketplace",
    "ecommerce",
    "books",
    "self-publishing",
    "amazon-alternative",
    "express",
    "api",
    "stripe"
  ],
  "author": "Travis Eric",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Traviseric/teneo-marketplace.git"
  },
  "bugs": {
    "url": "https://github.com/Traviseric/teneo-marketplace/issues"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "form-data": "^4.0.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.5",
    "openai": "^4.73.0",
    "pdfkit": "^0.17.1",
    "puppeteer": "^22.0.0",
    "sqlite3": "^5.1.7",
    "stripe": "^14.25.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
