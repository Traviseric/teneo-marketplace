{
  "name": "teneo-marketplace",
  "version": "1.0.0",
  "description": "Teneo Marketplace - Decentralized Book Network",
  "main": "marketplace/backend/server.js",
  "scripts": {
    "start": "node marketplace/backend/server.js",
    "dev": "nodemon marketplace/backend/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/__tests__"
    ],
    "testMatch": [
      "**/*.test.js"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "testTimeout": 10000,
    "modulePathIgnorePatterns": [
      "<rootDir>/marketplace/package.json"
    ],
    "moduleNameMapper": {
      "^stripe$": "<rootDir>/marketplace/backend/node_modules/stripe",
      "^axios$": "<rootDir>/marketplace/backend/node_modules/axios"
    },
    "forceExit": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "archiver": "^6.0.1",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cheerio": "^1.2.0",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.0",
    "nostr-tools": "^1.17.0",
    "openai": "^6.25.0",
    "sqlite3": "^5.1.6",
    "stripe": "^14.5.0"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.2.2"
  }
}
